<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Redirect Page</title>
    <script type="text/javascript">
        // Get the current URL path (everything after the domain)
        var path = window.location.pathname.substring(1);
        
        // Remove trailing slash if present
        path = path.replace(/\/$/, "");
        
        // Set default redirect to index.html if no path
        var redirectTo = "index.html";
        
        // Custom redirects for specific paths
        if (path === "skinimalism") {
            redirectTo = "skinimalism.html";
        }
        else if (path === "food") {
            redirectTo = "food.html";
        }
        else if (path === "style") {
            redirectTo = "style.html";
        }
        else if (path === "services") {
            redirectTo = "services.html";
        }
        else if (path === "learn") {
            redirectTo = "learn.html";
        }
        else if (path === "events") {
            redirectTo = "events.html";
        }
        // Add more paths as needed
        
        // Redirect to the appropriate page
        window.location.replace(redirectTo);
    </script>
</head>
<body>
    <p>Redirecting...</p>
</body>
</html> 